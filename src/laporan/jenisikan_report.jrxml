<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
              http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="jenisikan_report"
              pageWidth="595"
              pageHeight="842"
              columnWidth="555"
              leftMargin="20"
              rightMargin="20"
              topMargin="20"
              bottomMargin="20"
              language="java">

    <!-- QUERY -->
    <queryString>
        <![CDATA[
            SELECT id_ikan, id_penjual, nama_ikan, kategori, harga, stok
            FROM jenis_ikan
        ]]>
    </queryString>

    <!-- FIELDS -->
    <field name="id_ikan" class="java.lang.String"/>
    <field name="id_penjual" class="java.lang.String"/>
    <field name="nama_ikan" class="java.lang.String"/>
    <field name="kategori" class="java.lang.String"/>
    <field name="harga" class="java.lang.String"/>
    <field name="stok" class="java.lang.String"/>

    <!-- TITLE -->
    <title>
        <band height="40">
            <staticText>
                <reportElement x="0" y="5" width="555" height="30"/>
                <textElement textAlignment="Center">
                    <font size="16" isBold="true"/>
                </textElement>
                <text><![CDATA[LAPORAN DATA JENIS IKAN]]></text>
            </staticText>
        </band>
    </title>

    <!-- COLUMN HEADER -->
    <columnHeader>
        <band height="25">

            <staticText>
                <reportElement x="0" y="5" width="60" height="20"/>
                <textElement textAlignment="Center">
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[ID Ikan]]></text>
            </staticText>

            <staticText>
                <reportElement x="60" y="5" width="80" height="20"/>
                <textElement textAlignment="Center">
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[ID Penjual]]></text>
            </staticText>

            <staticText>
                <reportElement x="140" y="5" width="150" height="20"/>
                <textElement textAlignment="Center">
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[Nama Ikan]]></text>
            </staticText>

            <staticText>
                <reportElement x="290" y="5" width="90" height="20"/>
                <textElement textAlignment="Center">
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[Kategori]]></text>
            </staticText>

            <staticText>
                <reportElement x="380" y="5" width="85" height="20"/>
                <textElement textAlignment="Center">
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[Harga]]></text>
            </staticText>

            <staticText>
                <reportElement x="465" y="5" width="90" height="20"/>
                <textElement textAlignment="Center">
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[Stok]]></text>
            </staticText>

        </band>
    </columnHeader>

    <!-- DETAIL -->
    <detail>
        <band height="22">

            <textField>
                <reportElement x="0" y="3" width="60" height="16"/>
                <textElement textAlignment="Center"/>
                <textFieldExpression><![CDATA[$F{id_ikan}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="60" y="3" width="80" height="16"/>
                <textElement textAlignment="Center"/>
                <textFieldExpression><![CDATA[$F{id_penjual}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="140" y="3" width="150" height="16"/>
                <textFieldExpression><![CDATA[$F{nama_ikan}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="290" y="3" width="90" height="16"/>
                <textFieldExpression><![CDATA[$F{kategori}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="380" y="3" width="85" height="16"/>
                <textFieldExpression><![CDATA[$F{harga}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="465" y="3" width="90" height="16"/>
                <textElement textAlignment="Center"/>
                <textFieldExpression><![CDATA[$F{stok}]]></textFieldExpression>
            </textField>

        </band>
    </detail>

</jasperReport>
